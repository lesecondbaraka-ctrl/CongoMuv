# Frontend environment variables (Vite requires VITE_ prefix)
# Core backend API URL
VITE_API_URL=http://localhost:8080

# Auth / Supabase (REQUIRED - no mock mode)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Crypto
# Used to encrypt/decrypt sensitive fields (e.g., phone). Use a strong passphrase in prod
VITE_APP_ENCRYPTION_KEY=change-me-dev-secret

# Payments (optional)
VITE_FLUTTERWAVE_PUBLIC_KEY=
VITE_FLUTTERWAVE_SECRET_KEY=
VITE_AIRTEL_CLIENT_ID=
VITE_AIRTEL_CLIENT_SECRET=
VITE_ORANGE_CLIENT_ID=
VITE_ORANGE_CLIENT_SECRET=

# Email (optional)
VITE_MAILGUN_API_KEY=
VITE_MAILGUN_DOMAIN=
VITE_SENDGRID_API_KEY=

# SMS (optional)
VITE_TWILIO_ACCOUNT_SID=
VITE_TWILIO_AUTH_TOKEN=
VITE_TWILIO_PHONE_NUMBER=
VITE_INFOBIP_API_KEY=
